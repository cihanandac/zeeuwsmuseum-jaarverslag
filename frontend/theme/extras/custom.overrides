/* ***************************************
*********FOOTER BEGINNING ***************
*************************************** */
#footer {
  background-color: white;
  height: 400px;
  align-items: center;
  font-family: 'UniversLight', 'Helvetica Neue', Helvetica, sans-serif;
  line-height: 40px;
  letter-spacing: 0.05em;
  font-size: 17px;
  font-weight: 300;
  padding-top: 40px;
  border-top: 0.3px solid #c3c3c3;

  p {
    font-size: 16px;
  }

  a {
    font-size: 16px;
    color: inherit;
    text-decoration: none;
  }

  #top-footer {
    padding-bottom: 70px;
    height: auto;
    padding-top: 70px;

    .top-wrapper {
      padding: 0% 7%; //for sides
      display: flex;
      flex-direction: row;
      flex-wrap: wrap;
      justify-content: space-between;
      align-items: top;

      .footerInfoBox {
        margin-right: 2%;
        flex: 1 1 0 !important;
        padding-right: 45px;
        min-width: 260px;
        margin-top: 8px;
        margin-bottom: 8px;

        p#address {
          padding-bottom: 2px;
        }
        p#phoneNumber {
          padding-bottom: 0%;
          margin-bottom: 7px;
        }
        a#mailadress {
          margin-bottom: 9px;
          display: block;
        }
      }

      #footermail {
        // flex: 1 4 auto;
        border-left: 0.3px solid #c3c3c3;
        min-width: 300px;
        flex: 1.3 1.3 0;
        padding-left: 50px;
        margin-right: 0px;
        p {
          width: 100% !important;
          word-break: break-word;
        }

        dd.portletItem.odd {
          display: flex;
          justify-items: center;
          margin-left: 0;
          padding-top: 32px;

          form {
            width: 100%;
            padding-right: 0%;
          }
        }

        #form-widgets-email {
          border: 0.5px solid #c3c3c3;
          font-family: 'UniversLight', 'Helvetica Neue', Helvetica, sans-serif;
          line-height: 40px;
          letter-spacing: 0.05em;
          font-size: 17px;
          font-weight: 300;
          padding-top: 10px;
          height: 55px !important;
        }
        svg {
          padding-top: 5px;
        }

        div.input-group {
          display: flex;
          align-items: center;
          padding-top: 10px;

          input {
            border: 1px solid;
            flex-grow: 1;
            padding-left: 20px;
            /* width: 80%; */
            /* flex: 1 1 700px; */
          }

          span.input-group-btn {
            /* width: 15%; */
          }
          button.submit-button {
            width: 59px;
            height: 55px;
            background-color: #ffffff;
            color: #7bb0e7;
            border: 0.5px solid #7bb0e7;
            padding: 0% !important;
          }
        }
      }
    }
  }

  #bottom-footer {
    display: flex;
    align-content: center;
    justify-content: center;
    border-top: 0.3px solid #c3c3c3;
    align-items: center;
    padding-top: 125px;
    padding-bottom: 125px;
    width: 100%;
  }

  a.text-button {
    font-family: 'UniversCn', Arial, sans-serif;
    margin: 0 auto;
    display: block;
    border: 0.5px solid #7bb0e7;
    border-radius: 0px;
    text-transform: uppercase;
    color: #7bb0e7;
    letter-spacing: 0.1em;
    width: auto;
    height: 55px;
    font-size: 16px;
    background-color: transparent;
    letter-spacing: 0.05em;
    text-shadow: 0px 0px 0px #ffffff;
    font-weight: 400;
    text-align: center;
    padding-top: 5px;
  }
  input,
  button {
    /* margin-top: 30px; */
    height: 44px;
  }

  .social {
    color: #7bb0e7;
    width: 40px;
    height: 20px;
    margin-top: 25px;
  }
}

#mailadress {
  border-bottom: 1px solid #7bb0e7;
  width: 190px;
  height: 30px !important;
  padding-bottom: 0% !important;
  margin-bottom: 20px !important;
}
#mailadress:hover {
  color: #7bb0e7 !important;
  border-bottom: 1px solid #ffffff;
}
