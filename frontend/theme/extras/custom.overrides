/* FOOTER BEGINNING */
#footer{
  p{
    font-size: 16px;
  }
  a{
    font-size: 16px;
    color: inherit;
    text-decoration: none;
  }

  
  background-color: white;
  align-items: center;
  /* justify-content:center; */
  /* font-family: 'UniversLight', 'Helvetica Neue', Helvetica, sans-serif; */
  font-family: 'Quicksand', sans-serif;
  line-height: 40px;
  letter-spacing:0.05em;
  /* color: #000000; */
  font-size: 17px;
  font-weight: 300;

  padding-top: 40px;
  border-top: 0.3px solid #c3c3c3;
  
  #top-footer{
    /* margin-left: 10%; */
    margin-bottom: 40px;
    height: auto;
    /* width: 80%; */
    /* border: 1px black solid; */
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: space-around;
    align-items: top;
    padding-top: 90px;
    padding-bottom: 90px;

    /* background-color: blue; */

    .footerInfoBox{
      /* text-align: center; */
      /* margin-top: 5%; */   
      /* width: 350px;  */
      flex: 1 1 350px;
      width: 250px;
      padding: 25px 70px;
      p#address{
          padding-bottom: 2px;
      }



    }
    #footermail{ 
      /* padding-left: 10px; */
      border-left: 0.3px solid #c3c3c3;

      dd.portletItem.odd{
        display:flex;
        justify-items:center;
        margin-left:0;

        form{
          width: 100%;
        }
      }

      div.input-group{
        /* margin-top: 35px; */
        /* border: 1px solid; */
        display: flex;
        /* justify-content: center; */
        align-items: center;



        input{
          border:1px solid;
          width:  80%;
        }

        span.input-group-btn{
          width: 15%;
        }
        button.submit-button{
          width: 100%;
          background-color: #ffffff;
          color: #7bb0e7
        }

      }
    }
  }  

  #bottom-footer{
    display: flex;
    align-content: center;
    justify-content: center;

    border-top: 0.3px solid #c3c3c3;

    align-items: center;
    /* margin-left: 10%; */
    padding-top: 110px;
    padding-bottom: 110px;
    width: 100%;
    
    .ui.image {
      /* max-width: 50% !important; */
    }
  }


  a.text-button{
      /* font-family: 'UniversCn', Arial, sans-serif; */
      font-family: 'Quicksand', sans-serif;
      margin: 0 auto;
      /* margin-top: 0px; */
      display: block;
      /* padding: 8px; */
      /* padding-right: 17px; */
      /* padding-bottom: 17px; */
      /* padding-left: 17px; */
      border: 0.5px solid #7bb0e7;
      border-radius: 0px;
      text-transform: uppercase;
      color: #7bb0e7;
      letter-spacing: 0.1em;
      width: auto;
      height: 50px;
      font-size: 16px;
      background-color: transparent;
      letter-spacing: 0.05em;
      text-shadow: 0px 0px 0px #ffffff;
      font-weight: 400;
      /* padding-left: 28px; */
      /* padding-right: 28px; */
      /* padding-bottom: 12px; */
      text-align: center;
      padding-top: 5px;
      /* margin-bottom: 30px; */
    }
    input, button{
      margin-top: 30px;
      height: 40px;
    }

    .social{
      color: #7bb0e7;
      width: 30px;
    }

}
/* FOOTER END */




