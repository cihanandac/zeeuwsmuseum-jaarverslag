/* HEADER BEGIN */
.logo-written{
  navbar name css
  font-family: 'UniversCn', Arial, sans-serif;
  color: #7bb0e7;
  font-size: 30px;
  padding-top: 22px;
  padding-bottom: 35px;
}

.ui.basic.segment .header .logo-nav-wrapper {
  justify-content: space-between;
}

.ui.basic.segment.header-wrapper {
