/* ***DROPDOWN MENU*** */

div#dropdownmenu{
  /* width: 800px; */
  /* color: #000000; */
  a{
    text-decoration: none;
    color: #000000;
    font-size: 1em;
    text-transform: uppercase;
    /* font-family: 'UniversCn',Roboto, Arial, sans-serif; */
    font-family: 'Quicksand', sans-serif;
    font-weight: 500;
    letter-spacing: 0.05em;
  }
  svg{
    padding-top: 5px;
  }
  li.nav__submenu-item{
    display: none;
    width: 200px;
    padding-left: 5px;
    a{
      font-size: 10px;
    }
  }
  ul.nav__submenu{
    a{
      /* font-family: 'UniversCn', 'Helvetica Neue', Helvetica, sans-serif; */
      font-family: 'Quicksand', sans-serif;
      font-weight: 200;
      letter-spacing: 0.1em;
    }
    display: none;
    /* height: 30px; */
    text-align: center;
    /* width:210px; */
    /* margin-top:5px; */
    padding:0;
    background-color: rgba(260, 255, 255);
    line-height: 15px;
    border: 0.3px solid #c3c3c3 ;
  }

  li.nav__menu-item:hover{
    li.nav__submenu-item{
      display: inline-block;
      position:relative;
      text-align:left;
    }
    ul.nav__submenu{
      display: block;
    }
  }
  li.nav__menu-item{
      color:#000000;
      padding-right: 15px;
  }
  ul.nav__menu{
    display: flex;
    justify-content: center;
    flex-wrap: wrap;
    list-style-type: none;

  }
  .nav__submenu {
    font-weight: 300;
    text-transform: none;
    position: absolute;
    width: 200px;
  }

}
/* ***END OF DROPDOWN MENU*** */








@media only screen  and (max-width: 1080px){
  div#dropdownmenu{
    padding-left: 30%;
  }

  ul.nav__menu{
    flex-direction: column;
    width: 210px;
    padding-left: 5%;
  }
  li.nav__menu-item:hover{
    li.nav__submenu-item{
      display: block;
      text-align:left;
      margin-left: 5px;

    }
    ul.nav__submenu{
      display: block;
      background-color: white;
      line-height: 10px;
      /* height: 150px; */
      margin-left: 60px;
      z-index: 10;
    }
  }
}


@media only screen  and (max-width: 808px){

  div#dropdownmenu{
    width: 200px;
    padding: 0%;
  }
    li.nav__menu-item:hover{
    li.nav__submenu-item{
      display: block;
      text-align:left;
      margin-left: 5px;

    }
    ul.nav__submenu{
      display: block;
      background-color: white;
      line-height: 10px;
      /* height: 150px; */
      margin-left: 40px;
      z-index: 10;
    }
  }
}