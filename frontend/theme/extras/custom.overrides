  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding-top: 20px;
  padding-bottom: 20px;
}

.SocialbuttonsBottom {
  /* position: relative; */
  /* width: 150px; */
  height: 50px;
  /* left: 50px; */
  padding-top: 80px !important;
  align-self: center;
  list-style-type: none;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
